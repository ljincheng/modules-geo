<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUG">
<Appenders>
	<Console name="Console" target="SYSTEM_OUT">
		<PatternLayout pattern="%d{hh:mm:ss,SSS} [%t] %-5p %c %X - %m%n"/>
	</Console>
</Appenders>
<Loggers>
	<Logger name="org.apache.ibatis.level" level="INFO" additivity="false">
		<AppenderRef ref="Console"/>
	</Logger>
	<Logger name="javax.activation.level" level="INFO"/>
	<Root level="INFO">
		<AppenderRef ref="Console"/>
	</Root>
</Loggers>
</Configuration>

